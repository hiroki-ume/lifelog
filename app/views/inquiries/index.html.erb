<div class="row">
  <div class="col-lg-6">
    <h3>お問い合わせ一覧</h3>
    <% @inquiries.each do |inquiry| %>
      <table class="table table-default">
        <tr>
          <th>名前</th>
          <td><%= inquiry.name %></td>
        </tr>
        <tr>
          <th>メールアドレス</th>
          <td><%= inquiry.email %></td>
        </tr>
        <tr>
          <th>内容</th>
          <td><%= inquiry.content %></td>
        </tr>
        <tr>
          <th></th>
          <td></td>
        </tr>
      </table>
    <% end %>
  </div>
</div>
